    "react-dom": "^18.2.0",
    "react-router-dom": "^4.2.2",
    "save": "^2.9.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "styled-components": "^5.3.9",
    "typescript": "^5.0.2"
